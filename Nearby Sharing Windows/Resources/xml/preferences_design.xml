<?xml version="1.0" encoding="utf-8"?>
<androidx.preference.PreferenceScreen
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto">

	<SwitchPreference
        app:defaultValue="false"
        app:key="force_dark_mode"
        app:title="@string/force_dark_mode_title"
        app:summary="@string/force_dark_mode_summary"/>

	<Preference
        app:key="switch_language"
        app:title="@string/switch_language_title"
		app:summary="@string/switch_language_summary" />

</androidx.preference.PreferenceScreen>