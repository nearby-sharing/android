{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "../",
          "files": [ "lib/**/*.csproj" ]
        }
      ],
      "dest": "api/lib"
    },
    {
      "src": [
        {
          "src": "../",
          "files": [ "src/*.csproj" ]
        }
      ],
      "dest": "api/android"
    }
  ],
  "build": {
    "content": [
      {
        "files": "**/*.{md,yml}",
        "exclude": "_site/**/*"
      },
      {
        "src": "../",
        "files": [ "README.md", "CREDITS.md" ]
      }
    ],
    "resource": [
      {
        "src": "../",
        "files": [ "LICENSE" ]
      }
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_disableNextArticle": true,
      "_appLogoPath": "favicon.svg",
      "_appFaviconPath": "favicon.svg",
      "_appName": "NearShare",
      "_gitUrlPattern": "github"
    },
    "output": "_site",
    "template": [ "default", "modern" ]
  }
}